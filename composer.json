{
  "name": "nalingia/laravel-i18n",
  "description": "An opinionated Laravel package for model internationalisation.",
  "keywords": [
    "i18n",
    "orm",
    "model",
    "eloquent",
    "laravel",
    "multi-language",
    "multilingual",
    "laravel-i18n"
  ],
  "type": "library",
  "require": {
    "php": "^7.2",
    "laravel/framework": "~5.8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "orchestra/testbench": "^3.8.0",
    "mockery/mockery": "^1.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Gianmarco Nalin",
      "email": "gianma.nalin89@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Nalingia\\I18n\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nalingia\\I18n\\Tests\\": "./tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Nalingia\\I18n\\I18nServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
